# ASP.NET Core
ASPNETCORE_ENVIRONMENT=Production

# Database
ConnectionStrings__BaseDb=Host=<db-host>;Port=5432;Database=InfoSYSDb;Username=<db-user>;Password=<db-password>

# JWT
TokenOptions__SecurityKey=<replace-with-strong-random-key>

# Docker Compose credentials
POSTGRES_USER=infosys
POSTGRES_PASSWORD=<replace-with-strong-password>
POSTGRES_DB=InfoSYSDb
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=<replace-with-strong-password>

# Frontend API
NEXT_PUBLIC_API_URL=https://api.example.com
